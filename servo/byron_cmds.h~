// get values of sensors connected to BYRON
#define BYRON_CMD_POLL 				2

#define BYRON_CMD_LOWER_IN                      3
#define BYRON_CMD_LOWER_OUT                     4
#define BYRON_CMD_SET_SMOOTHING			7
#define BYRON_CMD_SET_PWM0			8
#define BYRON_CMD_SET_PWM1			9
#define BYRON_CMD_SET_PWM2			10
#define BYRON_CMD_SET_PWM3			11
#define BYRON_CMD_SET_PWM4			12
#define BYRON_CMD_SET_PWM5			13

// Start Bootloader for Software updates
#define BYRON_START_BOOTLOADER 	14



